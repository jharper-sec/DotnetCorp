@model IEnumerable<DotnetCorp.Models.UserModel>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
  <head>
    <title>Users</title>
  </head>
  <body>
    <form method="post" enctype="application/x-www-form-urlencoded">
      <input type="search" name="searchTermFromUser" />
      <button type="submit" name="btnSearchUsers">Search users</button>
    </form>
    @if (Model != null)
    {
      <table>
        <thead>
          <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Company</th>
            <th>Title</th>
            <th>Email Address</th>
            <th>Phone Number</th>
          </tr>
        </thead>
        @foreach (var user in @Model)
        {
          <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Company</td>
            <td>@user.Title</td>
            <td>@user.EmailAddress</td>
            <td>@user.PhoneNumber</td>
          </tr>
        }
      </table>
    }
  </body>
</html>